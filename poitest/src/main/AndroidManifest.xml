<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dstadler.poiandroidtest.poitest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="androidx.multidex.MultiDexApplication" >
        <activity
            android:name=".DocumentListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DocumentDetailActivity"
            android:label="@string/title_document_detail"
            android:parentActivityName=".DocumentListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DocumentListActivity" />
        </activity>
    </application>

</manifest>
